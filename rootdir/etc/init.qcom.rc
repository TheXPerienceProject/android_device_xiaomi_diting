on post-fs && property:ro.boot.hwc=CN
    # Remove euicc on CN Variant
    mount none /dev/null /product/etc/permissions/privapp-permissions-euiccgoogle.xml bind
    mount none /dev/null /product/etc/permissions/android.hardware.telephony.euicc.xml bind
    mount none /dev/null /product/priv-app/EuiccGoogle/EuiccGoogle.apk bind
    mount none /dev/null /product/app/XiaomiEuicc/XiaomiEuicc.apk bind

   # bind camera libs to fix camera issue on CN variant
   mount none /system/cncam/lib/camera/com.qti.sensormodule.diting_semco_s5khp1_wide.bin /vendor/lib/camera/com.qti.sensormodule.diting_semco_s5khp1_wide.bin  bind
   mount none /system/cncam/lib64/camera/com.qti.sensormodule.diting_semco_s5khp1_wide.bin /vendor/lib64/camera/com.qti.sensormodule.diting_semco_s5khp1_wide.bin  bind

on property:dev.bootcomplete=1
    chmod 0666 /dev/xiaomi-touch

on property:dev.bootcomplete=1 && property:persist.sys.diting.dt2w=*
    exec /system/bin/xiaomi-touch 14 ${persist.sys.diting.dt2w}

service vendor.audio-hal /vendor/bin/hw/android.hardware.audio.service_64
    override
    class hal
    user audioserver
    # media gid needed for /dev/fm (radio) and for /data/misc/media (tee)
    group audio camera drmrpc inet media mediadrm net_bt net_bt_admin net_bw_acct oem_2901 wakelock
    capabilities BLOCK_SUSPEND SYS_NICE
    # setting RLIMIT_RTPRIO allows binder RT priority inheritance
    rlimit rtprio 10 10
    ioprio rt 4
    task_profiles ProcessCapacityHigh HighPerformance
    onrestart restart audioserver
